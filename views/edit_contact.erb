<h1>Edit Contact</h1>

<form action='/contacts/<%= @contacts.id %>' method='post'>
  <input type='hidden' name='_method' value='put'>
  <input type='text' name='first_name' value='<%= @contacts.first_name %>'>
  <input type='text' name='last_name' value='<%= @contacts.last_name %>'>
  <input type='text' name='email' value='<%= @contacts.email %>'>
  <input type='text' name='note' value='<%= @contacts.note %>'>
  <input type='submit' value='Edit Contact'>
</form>
